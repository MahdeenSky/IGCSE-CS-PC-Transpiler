// Just a couple examples to be used for testing
m = 0
FOR i = 1 TO 6
FOR t = 1 TO 7
FOR l = 1 to 9
m = m + 1
NEXT l
NEXT t
NEXT i
PRINT m
// Correct Output 378

INPUT H
INPUT M
IF H > 0 THEN
PRINT "Outer"
IF M > 0 THEN
PRINT "Both Greater than 0"
ENDIF
ELSE
PRINT "Other"
ENDIF
PRINT "Done"

INPUT M
WHILE M > 0 DO
PRINT "M is POSITIVE"
INPUT M
INPUT H
WHILE H > 5 DO
PRINT "H Greater than 5"
INPUT H
ENDWHILE
ENDWHILE

INPUT H
WHILE H > 5 DO
PRINT "H Greater than 5"
INPUT H
ENDWHILE